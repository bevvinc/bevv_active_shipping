<div data-hook="admin_product_package_form_fields">
  <div class="six columns alpha">
    <div data-hook="length" class="field">
      <%= f.label I18n.t(:length, scope: :spree) %><br>
      <%= f.text_field :length %>
    </div>
    <div data-hook="width" class="field">
      <%= f.label I18n.t(:width, scope: :spree) %><br>
      <%= f.text_field :width %>
    </div>
  </div>
    <div class="six columns omega">
    <div data-hook="height" class="field">
      <%= f.label I18n.t(:height, scope: :spree) %><br>
      <%= f.text_field :height %>
    </div>
    <div data-hook="weight" class="field">
      <%= f.label I18n.t(:weight, scope: :spree) %><br>
      <%= f.text_field :weight %>
    </div>
  </div>
</div>

<div class="clear"></div>
